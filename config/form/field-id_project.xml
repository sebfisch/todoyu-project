<?xml version="1.0" encoding="UTF-8"?>
<form>
	
	<fieldsets>
		<fieldset name="main">
			<legend>Select project</legend>
			<elements>
				<field type="textinputAC" name="id_project">
					<label>project.project</label>
					<config>
						<acListener>
							<ext>project</ext>
							<controller>project</controller>
							<action>autocompletion</action>
						</acListener>
						<acLabelField>title</acLabelField>
						<options>
							<onCompleteCustom>Todoyu.Ext.project.Task.Edit.onProjectAutocomplete</onCompleteCustom>
						</options>
					</config>
					<required />
				</field>
			</elements>
		</fieldset>
	</fieldsets>
</form>