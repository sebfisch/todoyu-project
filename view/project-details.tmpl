<div id="project-{$idProject}-tabcontainer" class="data infoBoxOuter overflow">
	<div class="projectDetailRight">
		<div class="projectDetailLeft">
			<div class="projectDetailsTabs">
				{include file="project-details-tabs.tmpl"}
			</div>

			<div class="projectDetailsContent overflow" id="project-{$idProject}-tabcontent">
				<div id="project-{$idProject}-tabcontent-{$activeTab}" class="tab">
					{$content}
				</div>
			</div>
		</div>
	</div>
	<div id="project-{$idProject}-ajaxloader" class="ajaxLoader" style="display:none">{Label core.global.load}</div>
</div>